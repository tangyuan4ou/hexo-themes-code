.page-nav {
  width: 100%;
  height: 60px; 
  a {
    text-decoration: none;
  }
  .page-number {
    display: inline-block;
    margin: 10px 0 10px 10px;
    height: 40px;
    width: 40px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #ffffff;
    color: #ffffff;
  }
  .page-number:hover {
    background-color: #e9f1f6;
    color: #161823;
  }
  .extend {
    display: inline-block;
    margin: 10px 0 10px 10px;
    height: 40px;
    width: 70px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #ffffff;
    color: #ffffff;
  }
  .current {
    border: 0;
  }
  .current:hover {
    background-color: #161823;
    color: #e9f1f6;
  }
  .space {
    display: inline-block;
    margin-left: 10px;
    color: #ffffff;
  }
  .extend:hover {
    background-color: #e9f1f6;
    color: #161823;
  }
}